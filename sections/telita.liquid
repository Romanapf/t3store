<div>
  <h1>{{section.settings.title}}</h1>
  <p>{{section.settings.subtitle}}</p>
  </p>
  <a href="{{section.settings.url}}">{{section.settings.buttonText}}</a>
</div>

{% schema %}
{
  "name": "Section",
  "settings": [{
    "type": "text",
    "id": "title",
    "label": "Title",
    "default": "Title"
    }, {
    "type": "text",
    "id": "subtitle",
    "label": "Subtitle",
    "default": "Subtitle"
    }, {
    "type": "text",
    "id": "buttonText",
    "label": "button text",
    "default": "button text"
    }, {
    "type": "url",
    "id": "url",
    "label": "url",
    "default": "url"
  }],
  "presets": [{
    "name": "Telitaa"
  }]
}


{% endschema %}

