<div>
  <h1>{{section.settings.heading}}</h1>
  <p>{{section.settings.subheading}}</p>
  <a href='{{section.settings.url}}'>{{section.settings.CTA}}</a>
</div>

{% schema %}
{
"name": "Section name",
"settings": [{
"type": "text",
"id": "heading",
"label" : "Heading",
"default" : "My Custom Heading"
},
{
"type": "text",
"id": "subheading",
"label" : "Subheading",
"default" : "My Custom Subheading"
},
{
"type": "text",
"id": "CTA",
"label" : "CTA",
"default" : "CLICK ME"
},
{
  "type": "url",
  "id": "url",
  "label": "URL",
  "default": "https://www.t3store.com"
}],
"presets": [
{"name": "Banner 1"}
]
}
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

